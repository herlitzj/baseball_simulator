<p id="notice"><%= notice %></p>

<h1>Listing Lineups</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Team</th>
      <th>Player</th>
      <th>Position</th>
      <th>Inning in</th>
      <th>Inning out</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lineups.each do |lineup| %>
      <tr>
        <td><%= lineup.game_id %></td>
        <td><%= lineup.team_id %></td>
        <td><%= lineup.player_id %></td>
        <td><%= lineup.position %></td>
        <td><%= lineup.inning_in %></td>
        <td><%= lineup.inning_out %></td>
        <td><%= link_to 'Show', lineup %></td>
        <td><%= link_to 'Edit', edit_lineup_path(lineup) %></td>
        <td><%= link_to 'Destroy', lineup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lineup', new_lineup_path %>
