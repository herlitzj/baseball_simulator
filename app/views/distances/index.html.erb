<p id="notice"><%= notice %></p>

<h1>Listing Distances</h1>

<table>
  <thead>
    <tr>
      <th>Stadium</th>
      <th>Wind direction</th>
      <th>Lfl</th>
      <th>Lf</th>
      <th>Lc</th>
      <th>Cf</th>
      <th>Rc</th>
      <th>Rf</th>
      <th>Rfl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distances.each do |distance| %>
      <tr>
        <td><%= distance.stadium_id %></td>
        <td><%= distance.wind_direction %></td>
        <td><%= distance.lfl %></td>
        <td><%= distance.lf %></td>
        <td><%= distance.lc %></td>
        <td><%= distance.cf %></td>
        <td><%= distance.rc %></td>
        <td><%= distance.rf %></td>
        <td><%= distance.rfl %></td>
        <td><%= link_to 'Show', distance %></td>
        <td><%= link_to 'Edit', edit_distance_path(distance) %></td>
        <td><%= link_to 'Destroy', distance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distance', new_distance_path %>
